Cell["Check the relative scaling of nl and jl for 1<x<5:", "Text",ExpressionUUID->"f783280a-2a66-4636-a80a-02f66322c4d4"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"LogLogPlot", "[", 
  RowBox[{
   RowBox[{"Abs", "[", 
    RowBox[{
     RowBox[{"nl", "[", 
      RowBox[{"x", ",", "lval"}], "]"}], "/", 
     RowBox[{"jl", "[", 
      RowBox[{"x", ",", "lval"}], "]"}]}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "1", ",", "5"}], "}"}], ",", 
   RowBox[{"PlotRange", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"1", ",", 
      RowBox[{"10", "^", 
       RowBox[{"(", "20", ")"}]}]}], "}"}]}]}], "]"}]], "Input",ExpressionUUID\
->"ff064104-05f9-4257-b7a8-3c404fc9508d"],

